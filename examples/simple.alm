module simple
enums
   constant :: { c1, c2 } 
sorts
    obj :: universe
    a :: action
        attributes
            target : obj
    a1 :: a
    a2 :: a
fluents
    basic
        f : obj -> constant
axioms
    occurs(X) causes f(Obj) = c1 if
        instance(X, a1),
        X.target = Obj.

    occurs(X) causes f(Obj) = c2 if
        instance(X, a2),
        X.target = Obj.
